#PROGRAMAR LA SERIE FIBONACCI EN PYTHON.
def fibonacciA(n):
    fibonacci = [None, None, None, None, None, None, None, None, None, None, None]
    fibonacci[0]=1
    fibonacci[1]=1

    for i in range(2, n + 1):

        fibonacci[i] = fibonacci[i-2] + fibonacci[i-1]

    print("La serie fibonacci es:", fibonacci[n])

#declaras n
n=int(input("Ingresa el numero."))
fibonacciA(n)

